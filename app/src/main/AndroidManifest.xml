<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="neo.com.noibaiairport">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.MainActivity.MainActivity">

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.ActivityProfile">
            
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.ActivityChangeLanguage">

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Login.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.ActivityBooking">

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Fragment.FlightFragment.ActivityFlightDetail">

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.ActivityShops.ActivityShops">

        </activity>
        <service android:name=".Service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <activity
            android:screenOrientation="portrait"
            android:name=".View.Activity.Feedback.ActivityFeedback">

        </activity>
        <service android:name=".Service.MyFirebaseIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>